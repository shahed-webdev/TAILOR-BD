                    <CommandText>SELECT Dress.Dress_Name, SUM(OrderList.DressQuantity) AS Total_Dress ,SUM(OrderList.Pending_Work) AS Pending_Work,SUM(OrderList.WorkCompleteQuantity) AS WorkComplete,SUM(OrderList.ReadyForDeliveryQuantity) AS Pending_Delivery, SUM(OrderList.DeliveryQuantity) AS DeliveryQuantity,SUM(OrderList.Pending_Delivery) AS Total_Pending  FROM  [Order] INNER JOIN
